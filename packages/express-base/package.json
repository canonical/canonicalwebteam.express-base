{
  "name": "@canonical/express-base",
  "version": "0.0.0",
  "description": "Utility functions to create Expressjs apps with batteries included for Canonical's projects",
  "homepage": "https://github.com/canonical/canonicalwebteam.express-base",
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical/canonicalwebteam.express-base"
  },
  "bugs": {
    "url": "https://github.com/canonical/canonicalwebteam.express-base/issues"
  },
  "author": {
    "email": "webteam@canonical.com",
    "name": "Canonical Webteam"
  },
  "license": "LGPL-3.0-or-later",
  "type": "module",
  "module": "dist/esm/main.js",
  "types": "dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/main.js",
      "types": "./dist/types/main.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.types.json && vite build",
    "biome:format": "yarn exec -- biome format",
    "biome:format:fix": "yarn exec -- biome format --write",
    "biome:lint": "yarn exec -- biome lint",
    "biome:lint:fix": "yarn exec -- biome lint --write",
    "biome:check": "tsc -p tsconfig.types.json --noEmit && yarn exec -- biome check",
    "biome:check:fix": "tsc -p tsconfig.types.json --noEmit && yarn exec -- biome check --write",
    "test": "vitest run"
  },
  "dependencies": {
    "@canonical/express-middlewares": "0.0.0",
    "domhandler": "^5.0.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@canonical/biome-config": "^0.10.0-experimental.4",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
