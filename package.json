{
  "name": "express-base-packages",
  "private": true,
  "version": "0.0.1",
  "description": "Canonical's Express base implementation and middlewares",
  "author": {
    "email": "webteam@canonical.com",
    "name": "Canonical Webteam"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical/canonicalwebteam.express-base"
  },
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/canonical/canonicalwebteam.express-base/issues"
  },
  "homepage": "https://github.com/canonical/canonicalwebteam.express-base#readme",
  "workspaces": [
    "example",
    "packages/*"
  ],
  "scripts": {
    "dev": "lerna run dev --scope=example",
    "build": "lerna run build",
    "start": "lerna run start --scope=example",
    "biome:format": "lerna run format",
    "biome:format:fix": "lerna run format:fix",
    "biome:lint": "lerna run lint",
    "biome:lint:fix": "lerna run lint:fix",
    "biome:check": "lerna run check",
    "biome:check:fix": "lerna run check:fix",
    "test": "lerna run test",
    "publish": "lerna run build && lerna publish --no-private",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lerna": "^9.0.3",
    "nx": "22.3.3",
    "pinst": "^3.0.0"
  },
  "dependencies": {}
}
